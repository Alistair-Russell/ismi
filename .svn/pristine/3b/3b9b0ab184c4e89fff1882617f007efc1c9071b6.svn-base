/*********************************************************************************/
/* Header                                                                          */
/*********************************************************************************/

#header {
	width: 80%;
    margin: 0 auto;
    background-color: #FCF2DF;
    box-shadow: 0 0 5px 3px #D0D0D0;
    height: 100px;
    
    
}

#headerContent{
	color: #3B4186;
	display: inline-block;
    position: relative;
    top: 20%;
    left: 30px;
    
}

#headerContent a {
	font-size: xx-large;
	color: #3B4186;
}

#headerContent a:LINK {
	text-decoration: none;
	text-underline: none;
}


#headerContent p {
	padding: 0px;
	letter-spacing: 0.05em;
	font-size: 0.9em;
	
}


#icon {
	margin: 0 auto;
	width: 100%;
}

#iconContent{
	float: right;
	margin-top: 10px;
	margin-right: 100px;
}

