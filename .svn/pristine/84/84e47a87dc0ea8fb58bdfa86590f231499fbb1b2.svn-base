<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:a4j="http://richfaces.org/a4j"
	xmlns:rich="http://richfaces.org/rich">

<body>
	<ui:composition template="/templates/privateTemplate.xhtml">

		<ui:define name="privateContent">

			<h:panelGrid columns="1"
				rendered="#{Session.user != null}"
				styleClass="mainPanel">

					<rich:tabPanel id="tpEvents" 
						activeItem="#{Session.selectedEventTab}"
						itemChangeListener="#{Session.listenerEventTabChange}">
						
						<rich:tab header="Study" name="Study">
            				<ui:include src="studyEvent.xhtml" />
                		</rich:tab>
                		
                		<rich:tab header="Copy" name="Copy">
            				<ui:include src="copyEvent.xhtml" />
                		</rich:tab>
                		
                		<rich:tab header="Transfer" name="Transfer">
            				<ui:include src="transferEvent.xhtml" />
                		</rich:tab>
						
					</rich:tabPanel>
			</h:panelGrid>
		</ui:define>
	</ui:composition>
</body>

</html>
